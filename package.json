{
  "name": "emacs-plus",
  "main": "./lib/main",
  "version": "0.11.1",
  "description": "Emacs-like experience in the Atom.(Fork of atomic-emacs)",
  "repository": "https://github.com/aki77/atom-emacs-plus",
  "license": "MIT",
  "engines": {
    "atom": ">=1.6.0 <2.0.0"
  },
  "dependencies": {
    "underscore-plus": "1.x",
    "atom-package-deps": "4.6.0"
  },
  "package-deps": [
    "advanced-open-file"
  ],
  "keywords": [
    "emacs"
  ],
  "configSchema": {
    "activateMarkCommands": {
      "order": 1,
      "type": "array",
      "default": [
        "editor:select-word"
      ],
      "items": {
        "type": "string"
      }
    }
  }
}
